#include<iostream>
using namespace std;
int main() {
    string arr[2][2] = {{"apple", "banana"}, {"cat", "elephant"}};
    string longest = "";
    for(int i=0;i<2;i++) {
        for(int j=0;j<2;j++) {
            if(arr[i][j].size() > longest.size())
                longest = arr[i][j];
        }
    }
    cout << "Longest: " << longest << endl;
}
